{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["jest.base.cjs", "jest.config.cjs"],
  "pipeline": {
    "clean": {
      "cache": false
    },
    "prisma:generate": {
      "dependsOn": [],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "prisma:generate"],
      "outputs": ["dist/**", "node_modules/.cache/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "cache": false
    }
  }
}
