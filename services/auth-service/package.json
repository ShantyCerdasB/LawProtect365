{
  "name": "@lawprotect/auth-service",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch serverless.local.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --config ./jest.unit.cjs",
    "test:unit:coverage": "jest --coverage --config ./jest.unit.cjs",
    "test:integration": "jest --config ./jest.integration.cjs",
    "test:integration:coverage": "jest --coverage --config ./jest.integration.cjs",
    "test:integration:with-db": "npm run test:db:start && npm run test:integration && npm run test:db:stop",
    "test:db:start": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:stop": "docker-compose -f docker-compose.test.yml down",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.883.0",
    "@aws-sdk/client-eventbridge": "^3.864.0",
    "@aws-sdk/client-ssm": "^3.864.0",
    "@lawprotect/shared-ts": "^0.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4"
  }
}